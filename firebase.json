{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/web-vitals-tracker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/trackwebvital/**",
        "function": "trackWebVital"
      },
      {
        "source": "/trackwebvital/error",
        "function": "trackWebVitalError"
      }
    ]
  }
}
